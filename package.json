{
  "name": "testowo",
  "version": "1.0.0",
  "description": "Test practice app",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:feed": "node backend/seeder",
    "data:remove": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend",
    "test": "jest --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daniel-Badura/testowo.git"
  },
  "keywords": [
    "test"
  ],
  "author": "Daniel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Daniel-Badura/testowo/issues"
  },
  "homepage": "https://github.com/Daniel-Badura/testowo#readme",
  "dependencies": {
    "@google-cloud/storage": "^6.11.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "fix": "^0.0.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.3.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.3",
    "webpack": "^5.88.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^1.14.9"
  }
}
